{
  "name": "ngavinsir.com",
  "version": "0.0.1",
  "description": "ngavinsir.com",
  "scripts": {
    "build": "cross-env NODE_ENV=production sapper build --legacy --ext '.svelte .svx'",
    "cy:open": "cypress open",
    "cy:run": "cypress run",
    "dev": "sapper dev --ext '.svelte .svx'",
    "export": "cross-env NODE_ENV=production sapper export --legacy --ext '.svelte .svx'",
    "start": "node __sapper__/build",
    "test": "run-p --race dev cy:run"
  },
  "dependencies": {
    "compression": "1.7.4",
    "gray-matter": "^4.0.2",
    "polka": "next",
    "sirv": "1.0.6",
    "tinytime": "^0.2.6"
  },
  "devDependencies": {
    "@babel/core": "7.11.4",
    "@babel/plugin-syntax-dynamic-import": "7.8.3",
    "@babel/plugin-transform-runtime": "7.11.0",
    "@babel/preset-env": "7.11.0",
    "@babel/runtime": "7.11.2",
    "@fullhuman/postcss-purgecss": "2.3.0",
    "@rollup/plugin-babel": "5.2.0",
    "@rollup/plugin-commonjs": "15.0.0",
    "@rollup/plugin-node-resolve": "9.0.0",
    "@rollup/plugin-replace": "2.3.3",
    "@tailwindcss/typography": "^0.4.0",
    "autoprefixer": "9.8.6",
    "cross-env": "7.0.2",
    "cssnano": "4.1.10",
    "mdsvex": "^0.8.5",
    "npm-run-all": "4.1.5",
    "postcss": "7.0.32",
    "postcss-import": "12.0.1",
    "postcss-load-config": "2.1.0",
    "postcss-url": "8.0.0",
    "rollup": "2.26.5",
    "rollup-plugin-svelte": "5.2.3",
    "rollup-plugin-terser": "7.0.0",
    "sapper": "0.28.0",
    "svelte": "3.24.1",
    "svelte-image": "^0.2.9",
    "svelte-preprocess": "4.1.1",
    "tailwindcss": "2.0.2"
  }
